{
  "name": "playwrightdemo_ts",
  "version": "1.0.0",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "tsc-analysis": "npx tsc --noEmit",
    "prettier-analysis": "npx prettier --check .",
    "eslint-analysis": "npx eslint .",
    "madge-analysis": "npx madge --circular --extensions=ts ./",
    "code-analysis": "npm run tsc-analysis && npm run prettier-analysis && npm run eslint-analysis && npm run madge-analysis",
    "chromium-prod": "npx playwright test --project=chromium-prod",
    "firefox-prod": "npx playwright test --project=firefox-prod",
    "executionOrder": "npx playwright test --project=executionOrder --reporter=./src/reporters/ExecOrderReporter.ts",
    "robustnessVerification": "npx playwright test --project=*-prod --repeat-each=5 --reporter=dot --trace=retain-on-first-failure --quiet"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@eslint/js": "^9.31.0",
    "@faker-js/faker": "^9.9.0",
    "@playwright/test": "^1.54.2",
    "@types/node": "^24.0.13",
    "dotenv": "^17.2.0",
    "enforce-unique": "^1.3.0",
    "eslint": "^9.31.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-playwright": "^2.2.0",
    "madge": "^8.0.0",
    "prettier": "3.6.2",
    "typescript-eslint": "^8.37.0",
    "zod": "^4.1.1"
  },
  "dependencies": {
    "exceljs": "^4.4.0"
  },
  "overrides": {
    "glob": "^11.0.3",
    "fast-csv": "^5.0.5",
    "unzipper": "^0.12.3"
  }
}
