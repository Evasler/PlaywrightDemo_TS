  "description": "",
  "devDependencies": {
    "@eslint/js": "^9.31.0",
    "@faker-js/faker": "^9.9.0",
    "@playwright/test": "^1.50.1",
    "@types/node": "^24.0.13",
    "dotenv": "^17.2.0",
    "enforce-unique": "^1.3.0",
    "eslint": "^9.31.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-playwright": "^2.2.0",
    "madge": "^8.0.0",
    "prettier": "3.6.2",
    "typescript-eslint": "^8.37.0"
  },
