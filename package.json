{
  "name": "playwrightdemo_ts",
  "version": "1.0.0",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "tscVerification": "npx tsc --noEmit",
    "prettierVerification": "npx prettier --check .",
    "eslintVerification": "npx eslint .",
    "madgeVerification": "npx madge --circular --extensions=ts ./",
    "chromium-prod": "npx playwright test --project=chromium-prod",
    "firefox-prod": "npx playwright test --project=firefox-prod",
    "executionOrder": "npx playwright test --project=executionOrder --reporter=./src/reporters/ExecOrderReporter.ts",
    "automationRobustnessVerification": "npx playwright test --project=*-prod --workers=1 --repeat-each=5 --reporter=dot --trace=retain-on-first-failure --quiet"
  },
  "keywords": [],
  "author": "",
